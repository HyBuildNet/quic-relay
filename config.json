{
  "listen": "127.0.0.1:5520",
  "handlers": [
    {"type": "logsni"},
    {
      "type": "sni-router",
      "config": {
        "routes": {
          "test.local": "hybuild.net:5521"
        }
      }
    },
    {
      "type": "terminator",
      "config": {
        "listen": ":5522",
        "cert": "server.crt",
        "key": "server.key",
        "backend_mtls": true,
        "log_client_packets": 0,
        "log_server_packets": 50,
        "skip_client_packets": 5,
        "skip_server_packets": 0,
        "max_packet_size": 102400
      }
    },
    {"type": "forwarder"}
  ]
}
